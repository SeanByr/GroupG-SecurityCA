<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:controller="com.example.groupgsecurityca.Controllers.GroupChatController"
            prefHeight="396" prefWidth="478"
            xmlns:fx="http://javafx.com/fxml">
    <children>
        <Button fx:id="sendMsgBTN" layoutX="404" layoutY="338" text="Send"/>
        <TextField fx:id="sendMsgTF" layoutX="30" layoutY="338" prefHeight="26" prefWidth="361"/>
        <ScrollPane fx:id="clientGroupChatSP" layoutX="30" layoutY="70" prefHeight="256" prefWidth="418">
            <content>
                <VBox fx:id="clientVBox" prefHeight="248" prefWidth="412"/>
            </content>
        </ScrollPane>
        <Label fx:id="clientNameLB" layoutX="196" layoutY="26" text="Client">
            <font>
                <Font name="Arial Rounded MT Bold" size="23"/>
            </font>
        </Label>
    </children>

</AnchorPane>
