<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>
<AnchorPane fx:id="loginAP"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.groupgsecurityca.Controllers.LoginController"
            prefHeight="300" prefWidth="478">
    <Label layoutX="30" layoutY="80" text="Username">
        <font>
            <Font name="Arial Rounded MT Bold" size="23"/>
        </font>
    </Label>
    <TextField fx:id="usernameTF" layoutX="30" layoutY="110"
               prefWidth="420" prefHeight="26"/>

    <Label layoutX="30" layoutY="160" text="Password">
        <font>
            <Font name="Arial Rounded MT Bold" size="23"/>
        </font>
    </Label>
    <TextField fx:id="passwordTF" layoutX="30" layoutY="190"
               prefHeight="26" prefWidth="420"/>

    <Button fx:id="loginBTN" layoutX="210" layoutY="250" text="Login"/>

    <Label layoutX="196" layoutY="26" text="Login">
        <font>
            <Font name="Arial Rounded MT Bold" size="23"/>
        </font>
    </Label>

</AnchorPane>
